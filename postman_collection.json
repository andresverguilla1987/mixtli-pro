{
  "info": {
    "name": "Mixtli \u2013 CRUD + S3",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "base",
      "value": "https://mixtli-pro.onrender.com"
    },
    {
      "key": "id",
      "value": ""
    },
    {
      "key": "key",
      "value": ""
    }
  ],
  "item": [
    {
      "name": "00 - Salud",
      "request": {
        "method": "GET",
        "url": "{{base}}/salud"
      }
    },
    {
      "name": "10 - Upload",
      "request": {
        "method": "POST",
        "url": "{{base}}/api/upload",
        "body": {
          "mode": "formdata",
          "formdata": [
            {
              "key": "file",
              "type": "file",
              "src": []
            }
          ]
        }
      }
    },
    {
      "name": "20 - Files",
      "request": {
        "method": "GET",
        "url": "{{base}}/api/files"
      }
    },
    {
      "name": "30 - Presigned",
      "request": {
        "method": "GET",
        "url": "{{base}}/api/file-url?key={{key}}"
      }
    },
    {
      "name": "40 - Delete File",
      "request": {
        "method": "DELETE",
        "url": "{{base}}/api/file?key={{key}}"
      }
    },
    {
      "name": "Users - List",
      "request": {
        "method": "GET",
        "url": "{{base}}/api/users"
      }
    },
    {
      "name": "Users - Create",
      "request": {
        "method": "POST",
        "url": "{{base}}/api/users",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"nombre\": \"Demo\",\n  \"email\": \"demo_{{$timestamp}}@example.com\"\n}"
        }
      }
    },
    {
      "name": "Users - Update",
      "request": {
        "method": "PUT",
        "url": "{{base}}/api/users/{{id}}",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"nombre\": \"Demo Edit\"\n}"
        }
      }
    },
    {
      "name": "Users - Delete",
      "request": {
        "method": "DELETE",
        "url": "{{base}}/api/users/{{id}}"
      }
    }
  ]
}