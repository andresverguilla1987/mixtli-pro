{
  "info": {
    "name": "Mixtli HOTFIX Users",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Salud",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{baseUrl}}/salud",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "salud"
          ]
        }
      }
    },
    {
      "name": "Listar usuarios",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{baseUrl}}/api/users",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "api",
            "users"
          ]
        }
      }
    },
    {
      "name": "Crear usuario",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\"email\": \"andres@test.com\", \"password\": \"123456\"}"
        },
        "url": {
          "raw": "{{baseUrl}}/api/users",
          "host": [
            "{{baseUrl}}"
          ],
          "path": [
            "api",
            "users"
          ]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "baseUrl",
      "value": "https://mixtli-pro.onrender.com"
    }
  ]
}