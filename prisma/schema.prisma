// Ejemplo de schema.prisma
// Ajusta seg√∫n tu base de datos
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Usuario {
  id        Int     @id @default(autoincrement())
  nombre    String
  email     String  @unique
  createdAt DateTime @default(now())
}
