generator client {
  provider = "prisma-client-js"
  // Let Prisma choose the right binary. Avoid custom envs that break (enableTracing, etc.).
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id           Int      @id @default(autoincrement())
  name         String
  email        String   @unique
  passwordHash String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
